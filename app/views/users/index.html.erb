<div class = "container">
 <div class = "row">
    <%= render 'books/list', book: @book %>

    <div class = "col-lg-8">
      <table class = "table table-striped">
        <tr>
          <th>image</th>
          <th>name</th>
        </tr>
        <% @users.each do |user| %><!-- インスタンス変数books -->
          <tr>
            <td><%= image_tag user.get_profile_image(100,100) %></td>
            <td><%= user.name %></td>
            <td><%= link_to "Show", user_path(user.id) %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>